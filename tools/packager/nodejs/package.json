{
  "name": "wa-kit",
  "description": "WhatsApp Web Protocol Buffers",
  "version": "{{WA_VERSION}}",
  "private": false,
  "main": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./proto/*": {
      "types": "./proto/*_pb.d.ts",
      "default": "./proto/*_pb.js"
    }
  },
  "engines": {
    "node": ">=16.13.2"
  },
  "keywords": [
    "whatsapp",
    "protobuf",
    "graphql",
    "typescript"
  ],
  "license": "MIT",
  "author": "Jo√£o Vitor Machado <joaov@zapctl.dev>",
  "homepage": "https://github.com/zapctl/wa-kit",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zapctl/wa-kit.git"
  },
  "bugs": {
    "url": "https://github.com/zapctl/wa-kit/issues"
  },
  "dependencies": {
    "@bufbuild/protobuf": "1.10.0"
  }
}